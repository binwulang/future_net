#include <iostream>
#include <queue>
#include <vector>
#include <algorithm>
#include <functional>
using namespace std;
class my{
public:
	int id;
	int data;
	my(int data,int id){
		this->data=data;
		this->id=id;
	}

};
class cmp{
public:
	bool operator()(my a,my b){
		return a.data>b.data;
	}
};
bool compare(const my &o1,const my &o2){
	return o1.data<o2.data;
}
int main()
{
	my a1(1,2);
	my a2(2,1);
	my a3(3,4);
	my a4(5,3);
	vector<my> test;
	test.push_back(a1);
	test.push_back(a2);
	test.push_back(a3);
	test.push_back(a4);
	sort(test.begin(),test.end(),compare);
	a1.data=5;
	for(my temp:test){
		cout<<temp.id<<","<<temp.data<<endl;
	}
	return 0;
}


