#ifndef SRC_SOLUTIONDFS_H_
#define SRC_SOLUTIONDFS_H_
#include <set>
#include "Graph.h"
class Solution_DFS {
	vector<int> path ;
	int Min_Cost;
	set<int> *IncVertexs;
public:
	Solution_DFS();
	virtual ~Solution_DFS();
	vector<int> * GetPath();
	void DFS(int StartID, int EndID,set<int> *IncVertexs,
			Graph *graph,int Cost,vector<int>* path);
	void FindPath(int StartID, int EndID
			,set<int>* IncVertexs,Graph *graph);
};

#endif /* SRC_SOLUTIONDFS_H_ */
